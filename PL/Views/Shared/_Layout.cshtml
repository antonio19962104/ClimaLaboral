    <script>
        $(document).ready(function () {
            var ancho = $("#u6071").width() / $('#u6071').parent().width() * 100;
            //f es el ancho de color en la barra de progreso
            if (ancho < 21) {//0 a 20
                $('#u6071').css('background-color', '#f15a24');
            }
            if (ancho > 20 && ancho < 41) {//21 a 40
                $('#u6071').css('background-color', '#f7931e');
            }
            if (ancho > 40 && ancho < 61) {//41 a 60
                $('#u6071').css('background-color', '#cccc01');
            }
            if (ancho > 60 && ancho < 81) {//61 a 80
                $('#u6071').css('background-color', '#8cc63f');
            }
            if (ancho > 80 && ancho < 101) {//81 a 100
                $('#u6071').css('background-color', '#39b54a');
            }

            var es_ie = navigator.userAgent.indexOf("Trident") > -1;
            if (es_ie) {
                $('.logoCL').css('position', 'relative');
                $('.imgFotter').css('position', 'relative');

                //Ajuste RadioButton
                $('#DEMO').css('visibility', 'visible');
                $('#DEMO').css('background-color', 'transparent');
                $('#DEMO').css('border', 'border:5px solid aqua');
            }
            window.addEventListener("orientationchange", function () {
                console.log("the orientation of the device is now " + screen.orientation.angle);
                document.location.reload(true);
            });
        });
    </script>
