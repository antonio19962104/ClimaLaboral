@model ML.Result
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/Reporte.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
    <title>ViewPrueba</title>
</head>
<script src="~/scripts/jquery-3.4.1.min.js"></script>
<body>
    
    <div class="headerSuperiorR" style="">
        <img src="~/images/3 Logo.png" class="logoCLR" />
    </div>
    <div class="container" style="padding-top:135px; margin-left:15px;">


        <p>Reporte Clima Laboral</p>
        <br />
        <p style="font-weight:100">Use los filtros para obtener informacion detallada</p>
        <br />
        @*<a href="@Url.Action("ReporteGetAll", "Home")">*@
        <input type="submit" name="GetAll" value="Ver reporte global" class="btn btn-danger" style="font-weight:bold;" onclick="$('#progressbar').css('display', 'none'); $('#myTable tbody').remove();  GetAllResult(); " />
        @*</a>*@
        <br /><br />
        <select name="OpcioneFiltro" id="optionFilter">
            <option value="0">Seleccionar</option>
            <option value="1">Por Id de Empleado</option>
            <option value="2">Por Unidad de negocio</option>
            <option value="3">Por Direccion</option>
            <option value="4">Por Area</option>
            <option value="5">Por Departamento</option>
        </select>
        <br /><br />


        <div id="GetByIdEmpleado" style="display:none;">
            <p style="">Filtrar por el Id de Empleado</p>
            @Html.TextBoxFor(model => model.Empleado.IdEmpleado, htmlAttributes: new { @placeholder = "Ingresa el id del empleado", @class = "form-control", @style = "display:inline;", @id = "IdEmpleado" })
            <br /><br />
            <input type="submit" value="Buscar" class="btn btn-danger" onclick="$('#progressbar').css('display', 'none'); $('#myTable tbody').remove(); getByIdEmpleado()" />
        </div>

        <div action="" id="GetByUnidadNegocio" style="display:none;">
            <p style="">Filtrar por Unidad de negocio</p>
            <select id="DDLUnidad" name="DDLCompanyCategoria">
                <option value="0">Selecciona Unidad de negocio</option>
                <option value="AUTOFINANCIAMIENTO">AUTOFINANCIAMIENTO</option>
                <option value="AUTOMOTRIZ">AUTOMOTRIZ</option>
                <option value="CORPORATIVO">CORPORATIVO</option>
                <option value="FINANCIERA">FINANCIERA</option>
                <option value="INTERNACIONAL">INTERNACIONAL</option>
                <option value="MANUFACTURA">MANUFACTURA</option>
                <option value="OTROS NEGOCIOS">OTROS NEGOCIOS</option>
                <option value="PRESIDENCIA">PRESIDENCIA</option>
                <option value="TURISMO">TURISMO</option>
            </select>
            <input type="submit" value="Buscar" class="btn btn-danger" onclick="$('#progressbar').css('display', 'none'); $('#myTable tbody').remove(); GetByUnidad()" />
        </div>

        <div id="GetByCompany" style="display:none;">
            <p>Filtrar por Direccion</p>
            <select id="CompanyCategoria" name="CompanyCategoria">
                <option value="0">Selecciona unidad de negocio</option>
                <option value="AUTOFINANCIAMIENTO">AUTOFINANCIAMIENTO</option>
                <option value="AUTOMOTRIZ">AUTOMOTRIZ</option>
                <option value="CORPORATIVO">CORPORATIVO</option>
                <option value="FINANCIERA">FINANCIERA</option>
                <option value="INTERNACIONAL">INTERNACIONAL</option>
                <option value="MANUFACTURA">MANUFACTURA</option>
                <option value="OTROS NEGOCIOS">OTROS NEGOCIOS</option>
                <option value="PRESIDENCIA">PRESIDENCIA</option>
                <option value="TURISMO">TURISMO</option>
            </select>
            <br /><br />
            @Html.DropDownList("Company", new SelectList(string.Empty, "Text", "Text"), "Selecciona Direccion", new { @onclick = "", @id = "Company", @class = "DDL classic", @width = 212 })@*Id deñ elemento Subcategoria*@
            <br /><br />
            @Html.DropDownList("Area", new SelectList(string.Empty, "Value", "Text"), "Selecciona una respuesta", new { @onclick = "", @id = "Area", @class = "DDL classic", @width = 212 })@*Id deñ elemento Subcategoria*@
            <br /><br />
            @Html.DropDownList("Departamento", new SelectList(string.Empty, "Value", "Text"), "Selecciona una respuesta", new { @id = "Departamento", @class = "DDL classic", @width = 212 })@*Id deñ elemento Subcategoria*@
            <br />
            <input id="BuscarByCompany" type="submit" value="Buscar" class="btn btn-danger" onclick="$('#progressbar').css('display', 'none'); $('#myTable tbody').remove(); GetByCompany()" />
            <input id="BuscarByArea" type="submit" value="Buscar" class="btn btn-danger" onclick="$('#progressbar').css('display', 'none');  $('#myTable tbody').remove(); GetByArea()" />
            <input id="BuscarByDepartamento" type="submit" value="Buscar" class="btn btn-danger" onclick="$('#progressbar').css('display', 'none');  $('#myTable tbody').remove(); GetByDepartamento()" />

        </div>


        <br />
        <p id="descripcion">Ultimo evento: @ViewBag.Event</p>
        <p id="datetime">Fecha/hora: @DateTime.Now</p>

        <div id="progressbar" class="progress" style="margin: 10px; width: 700px; display:none">
            <div id="dynamic" class="progress-bar progress-bar-success progress-bar-striped active" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                <span id="current-progress"></span>
            </div>
        </div>

        <hr style="" />
        <div class="row" style="padding-top:30px; display:none;">
            <div class="col-md-12 col-sm-3">
                <input type="submit" value="Exportar a excel" class="btn btn-success" onclick="tableToExcel('myTable', 'Reporte ClimaLaboral')" style="font-weight:bold;" onload="alert('ready');" />
            </div>
        </div>
    </div>

