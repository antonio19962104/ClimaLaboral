@model ML.Encuesta

@{
    Layout = null;
}

<!DOCTYPE html>

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    @Styles.Render("~/bundles/cssAdmin")

    <!-- Title -->
    <title>Bienvenido | Diagnostic 4U</title>
    <style>
        #header > .navbar {
            background-color: #000080;
        }

        #header-survey {
            background-color: #000080;
            height: 106px;
        }
    </style>
</head>
<body>
    <main id="wrapper">
        <header id="header-survey"><nav class="container-fluid"><div class="row center-vertically"><div class="col-4 col-sm-4 col-lg-4 col-xl-3"><img class="img-fluid d-none d-sm-block" alt="Logo Climalaboral" src="data:image/png;base64,"><img class="img-fluid d-xl-none d-lg-none d-md-none d-sm-none d-block" alt="Logo Climalaboral" src="data:image/png;base64," data-filename="logo-clima-laboral-sm.png"></div></div></nav></header>
        <div class="text-slide">

            @if (Model.Agradecimiento != "")
            {
                @Html.Raw(Model.Agradecimiento)
            }
            else {

                <h2 class="title-message">Agradecemos tu cooperación</h2>
                <p class="desc-message">pronto tendrás más noticias</p>
            }


        </div>
        <section id="slider">
            <!-- Swiper -->
            <div class="swiper-container">
                <div class="swiper-wrapper">
                    <div class="swiper-slide" style="background-image: url('../../img/background/bg-login-1.jpg');"></div><!--.swiper-slide-->
                    <div class="swiper-slide" style="background-image: url('../../img/background/bg-login-2.jpg');"></div><!--.swiper-slide-->
                    <div class="swiper-slide" style="background-image: url('../../img/background/bg-login-1.jpg');"></div><!--.swiper-slide-->
                    <div class="swiper-slide" style="background-image: url('../../img/background/bg-login-2.jpg');"></div><!--.swiper-slide-->
                </div><!--.swiper-wrapper-->
            </div><!--.swiper-container-->
        </section>


    </main>


    @Scripts.Render("~/bundles/jqueryAdmin")


    <script src="~/scripts/swiper.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    <script>
        var swiper = new Swiper('.swiper-container', {
            spaceBetween: 0,
            effect: 'fade',
            loop: true,
            centeredSlides: true,
            autoplay: {
                delay: 3500,
                disableOnInteraction: false,
            }
        });
    </script>


</body>
</html>
