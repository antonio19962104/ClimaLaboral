@model ML.Encuesta
@{
    Html.EnableClientValidation(true);
    Html.EnableUnobtrusiveJavaScript(true);
}
@{
    ViewBag.Title = Model.Nombre;
    Layout = null;
}
<!DOCTYPE html>

<html lang="es-es" style="overflow-x:hidden;">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title</title>
    <script src="~/scripts/jquery-1.12.4.min.js"></script>
    <link href="~/css/formProgressBar.css" rel="stylesheet" />
    <script src="~/scripts/formProgressBar.jquery.js"></script>
    @Styles.Render("~/bundles/cssEncuesta")
    <style type="text/css">
        .card-question textarea {
            display: flex;
            width: 100%;
        }
    </style>
    <script type="text/javascript">
        $( document ).ready(function() {
            function getAllPreguntasVisible() {
                //Update All items
                var formElements = $('#formularioEncuesta').find('input,textarea,select').filter('[required]:visible').toArray();
                arr = [];
                formElements.map(function (item) {
                    arr[item.name] = 0;
                    if (item.checked)
                        arr[item.name] = 1;
                });
                console.log(Object.keys(arr).length);
                var longitudActualizada = Object.keys(arr).length;
                return longitudActualizada;
            }


            $("#formularioEncuesta").formProgressBar({
                percentCounting: true, // default: false
                height: 25, // default: 10
                // default: 500
                transitionTime: 2000,

                //ease, linear, ease-in, ease-out, ease-in-out
                transitionType: 'ease'
            });

            //$('#jQueryProgressFormBar').hide();

            $('div .progress').attr('id', 'jQueryProgressFormBar');

            $('.progress-bar').css('transition', 'all 2000ms ease 0s');


            elem = $(".progress-bar")[0];

            let resizeObserver = new ResizeObserver(() => {
                //alert("The element was resized");
                //var porcentaje = $(".progress-bar").width();
                var width = $('.progress-bar').width();
                var parentWidth = $('.progress-bar').offsetParent().width();
                var percent = Math.round(100 * width / parentWidth);

                $('.progress-number').text(percent + '%');
            });

            resizeObserver.observe(elem);


        });
    </script>

    @{
//ViewBag.Perfil = Session["PerfilAdminLog"];
    }
</head>
