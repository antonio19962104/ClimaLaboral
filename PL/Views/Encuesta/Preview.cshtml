                                                        });
                                                    }
                                                });
                                                var temp = conf.filter(function (configuracion) {
                                                    return configuracion.IdPregunta == idPregunta && configuracion.IdRespuesta != idRespuesta;
                                                });
                                                [].forEach.call(temp,function (result) {
                                                    var itemHtml = document.getElementById("Bloque_Pregunta_"+ result.IdPreguntaOpen);
                                                    if(itemHtml.children.length > 1 && itemHtml.style.display != 'none'){
                                                        totalHabilitadas = totalHabilitadas - 1;
                                                    }
                                                    document.getElementById("Bloque_Pregunta_"+ result.IdPreguntaOpen).style.display="none";
                                                    
                                                    var buscaInputsT1 = document.querySelectorAll("#Bloque_Pregunta_"+ result.IdPreguntaOpen+" input[type=radio]" );
                                                    buscaInputsT1.forEach(function(inputsResT1) {
                                                        $('#'+inputsResT1.id+'').removeAttr("checked");
                                                        console.log(inputsResT1.id);
                                                    });
                                                });
                                            }
                                            htmlelem.style.display = 'block';
                                            if(htmlelem.children.length > 1){
                                                totalHabilitadas = totalHabilitadas + 1
                                            }
                                        }
                                        showNumeralAfterShowQuestions();
                                        numCallbackRuns++;
                                    })
                                })
                            }else{
                                var preguntasO = conf.filter(function (element) {
                                    if (isCheckBox != undefined && isCheckBox) {
                                        return element.IdPregunta == idPregunta && element.IdRespuesta == idRespuesta
                                    }else{
                                        return element.IdPregunta == idPregunta
                                    }
                                });
                                preguntasO.forEach(function (config) {
                                    if (isCheckBox != undefined || isCheckBox) {
                                        var hiddenf = document.querySelectorAll("#Bloque_Pregunta_"+config.IdPreguntaOpen);
                                        [].forEach.call(hiddenf,function (htmlelem) {
                                            if (htmlelem.style.display != 'none' && htmlelem.children.length > 1) {
                                                totalHabilitadas = totalHabilitadas - 1;
                                            }
                                            htmlelem.style.display = 'none';
                                        })
                                    }
                                    showNumeralAfterShowQuestions();
                                })
                            }
                        }
                    }
