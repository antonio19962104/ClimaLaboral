<script>
    //Llamada  a auitenticar
    function Autenticar() {


        var Password = $('#Pass').val();

        if (Password.length < 1) {

            swal({
                title: "Notificacion",
                text: "La clave de acceso no puede estar vacia!",
                icon: "warning",
                showCancelButton: false,
                showConfirmButton: true,
                //buttons: true,
                dangerMode: true,
            });

        }
        else {

            //Enviar OBJ ML.Empleado

            var listaJSOn = '{"ClavesAcceso":{"ClaveAcceso":"' + Password + '"}}'

            $.ajax({
                type: 'POST',
                data: listaJSOn, traditional: true,//Aqui era usuarioRespuestas
                url: '@Url.Action("Autenticar", "Home")',
                contentType: 'application/json; charset=utf-8',
                success: function (Response) {

                    //alert('Hello welcome')
                    if (Response == "Clave de acceso no válida") {

                        swal({
                            title: "Notificacion",
                            text: "Clave de acceso no valida",
                            icon: "warning",
                            showCancelButton: false,
                            showConfirmButton: true,
                            //buttons: true,
                            dangerMode: true,
                        });

                    }
                    else
                        if (Response == "Ya has realizado la encuesta!") {

                            swal({
                                title: "Notificacion",
                                text: "Ya has realizado la encuesta!",
                                icon: "warning",
                                showCancelButton: false,
                                showConfirmButton: true,
                                //buttons: true,
                                dangerMode: true,
                            });

                        }
                        else
                        if (Response == "Error del proveedor subyacente en Open.") {

                            swal({
                                title: "Notificacion",
                                text: "Error del proveedor subyacente en Open.",
                                icon: "warning",
                                showCancelButton: false,
                                showConfirmButton: true,
                                //buttons: true,
                                dangerMode: true,
                            });

                        }
                        else
                            if (Response == "Success") {

                                //LLevarme a Inicio
                                window.location = "Home/Inicio";

                            }
                            else
                            {
                                alert(Response);
                            }
                    


                }
            });
            function finished(err) {

                swal({
                    title: "Notificacion",
                    text: "Ha ocurrido un error al realizar la autenticacion!",
                    icon: "warning",
                    showCancelButton: false,
                    showConfirmButton: true,
                    //buttons: true,
                    dangerMode: true,
                });

            }

        }


    }


</script>


