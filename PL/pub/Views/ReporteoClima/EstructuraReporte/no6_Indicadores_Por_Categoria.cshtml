                                    <center><img ng-src="{{ vm.setIcono(item) }}" class="mt-2 {{ vm.setIcono(item, 1) }}"></center>
                                    <div class="clima-porcentaje robotoblack porcentaje-clima">
                                        <p class="mb-n2 f-15">{{ item }}%</p>
                                        <p ng-hide="!vm.hasHistorico" class="{{ vm.getClassDiff(item - vm.listDataHistoricoEE[0].NivelParticipacion) }} f-17 mb-1">{{ vm.getPorcentDiff(item, vm.listDataHistoricoEE[0].NivelParticipacion) }}%</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--#endregion categorias-->
                </div>
                <div class="d-flex justify-content-center margin-reporte">
                    <div class="col-12 p-0 m-0">
                        <img src="/img/ReporteoClima/indicadores.png" class="m-0 p-0" style="width:inherit;">
                    </div>
                </div>
            </div>
        </div>
    </div>
